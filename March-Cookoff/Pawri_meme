import java.util.*;

public class Main
{
	public static void main(String[] args) {
		//System.out.println("Hello World");
		int t;
		Scanner ob=new Scanner(System.in);
		t=ob.nextInt();
		while(t-->0)
		{
		    String s;
		    s=ob.next();
		    String ans=s.replaceAll("party","pawri");
		    System.out.println(ans);
		}
	}
}

